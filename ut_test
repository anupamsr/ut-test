#!/usr/bin/perl

use strict;
use warnings;

use FindBin;
use lib "$FindBin::RealBin"; # use the location of script

# Check if BUILDTOP is set
die 'BUILDTOP is not set' unless defined $ENV{BUILDTOP};

die '1 and only 1 argument is needed' if scalar @ARGV > 1;

exec "$FindBin::RealBin/ut_test_launcher", $FindBin::Script, @ARGV;
